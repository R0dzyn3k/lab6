<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="329.0" prefWidth="367.0" spacing="20.0" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.wsiz.lab3.PrimaryController">
   <children>
      <HBox prefHeight="246.0" prefWidth="425.0">
         <children>
            <TableView fx:id="tabela" prefHeight="246.0" prefWidth="369.0">
              <columns>
                <TableColumn fx:id="col_id" prefWidth="75.0" text="Id" />
                <TableColumn prefWidth="251.0" text="Dane">
                     <columns>
                        <TableColumn fx:id="col_imie" prefWidth="75.0" text="Imie" />
                        <TableColumn fx:id="col_nazwisko" prefWidth="75.0" text="Nazwisko" />
                     </columns>
                  </TableColumn>
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="inputImie">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </TextField>
            <TextField fx:id="inputNazwisko">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </TextField>
            <Button mnemonicParsing="false" onAction="#dodajObobe" text="Dodaj">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
